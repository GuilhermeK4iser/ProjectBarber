<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/colorview">

    <View
        android:id="@+id/view16"
        android:layout_width="205dp"
        android:layout_height="195dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="@+id/view8"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/view8"
        app:layout_constraintVertical_bias="0.882" />

    <View
        android:id="@+id/view15"
        android:layout_width="206dp"
        android:layout_height="195dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="@+id/view8"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/view8"
        app:layout_constraintVertical_bias="0.882" />

    <androidx.appcompat.widget.Toolbar
        android:id="@+id/toolbar3"
        style="@style/color"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="197dp"
        android:layout_height="40dp"
        android:fontFamily="@font/sfpro"
        android:text="@string/inicio"
        android:textAlignment="center"
        android:textColor="@color/white"
        android:textSize="25sp"
        app:layout_constraintBottom_toBottomOf="@+id/toolbar3"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="@+id/toolbar3"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.304" />

    <ImageView
        android:id="@+id/imageView6"
        android:layout_width="51dp"
        android:layout_height="55dp"
        app:layout_constraintBottom_toBottomOf="@+id/toolbar3"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/toolbar3"
        app:layout_constraintTop_toTopOf="@+id/toolbar3"
        app:layout_constraintVertical_bias="0.0"
        app:srcCompat="@drawable/barrademenu" />

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="52dp"
        android:layout_height="56dp"
        app:layout_constraintBottom_toBottomOf="@+id/toolbar3"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="@+id/toolbar3"
        app:layout_constraintTop_toTopOf="@+id/toolbar3"
        app:layout_constraintVertical_bias="0.0"
        app:srcCompat="@drawable/notificacao" />

    <View
        android:id="@+id/paraagendmnts_view"
        android:layout_width="186dp"
        android:layout_height="179dp"
        android:background="@drawable/buttonopc"
        android:clickable="true"
        android:onClick="abrirAgendmntsViewHome"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="@+id/view8"
        app:layout_constraintHorizontal_bias="0.798"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/view8"
        app:layout_constraintVertical_bias="0.439"
        tools:ignore="SpeakableTextPresentCheck" />

    <View
        android:id="@+id/view8"
        android:layout_width="411dp"
        android:layout_height="637dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="1.0" />

    <ImageView
        android:id="@+id/icone_listaagendmnts"
        android:layout_width="82dp"
        android:layout_height="88dp"
        android:layout_marginEnd="52dp"
        android:layout_marginBottom="4dp"
        android:src="@drawable/listar_agendmnts"
        app:layout_constraintBottom_toTopOf="@+id/listaragendmnts_texto"
        app:layout_constraintEnd_toEndOf="@+id/paraagendmnts_view" />

    <View
        android:id="@+id/accodata_view"
        android:layout_width="178dp"
        android:layout_height="198dp"
        android:background="@drawable/buttonopc"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="@+id/view8"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/view8"
        app:layout_constraintVertical_bias="0.0" />

    <View
        android:id="@+id/accopts_view"
        android:layout_width="188dp"
        android:layout_height="199dp"
        android:background="@drawable/buttonopc"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="@+id/view8"
        app:layout_constraintHorizontal_bias="0.808"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/view8"
        app:layout_constraintVertical_bias="0.0" />

    <ImageView
        android:id="@+id/opts_img"
        android:layout_width="143dp"
        android:layout_height="112dp"
        app:layout_constraintBottom_toTopOf="@+id/paraagendmnts_view"
        app:layout_constraintEnd_toEndOf="@+id/accopts_view"
        app:layout_constraintHorizontal_bias="0.446"
        app:layout_constraintStart_toEndOf="@+id/accodata_view"
        app:layout_constraintTop_toTopOf="@+id/accopts_view"
        app:layout_constraintVertical_bias="0.233"
        app:srcCompat="@drawable/saida" />

    <ImageView
        android:id="@+id/imageView3"
        android:layout_width="143dp"
        android:layout_height="112dp"
        app:layout_constraintBottom_toTopOf="@+id/paraagendar_view"
        app:layout_constraintEnd_toEndOf="@+id/accodata_view"
        app:layout_constraintHorizontal_bias="0.432"
        app:layout_constraintStart_toStartOf="@+id/accodata_view"
        app:layout_constraintTop_toTopOf="@+id/accodata_view"
        app:layout_constraintVertical_bias="0.233"
        app:srcCompat="@drawable/imgusuario" />

    <TextView
        android:id="@+id/usuario_texto"
        android:layout_width="159dp"
        android:layout_height="40dp"
        android:fontFamily="@font/sfpro"
        android:text="@string/pagina_usuario"
        android:textAlignment="center"
        android:textColor="@color/white"
        android:textSize="20sp"
        app:layout_constraintBottom_toTopOf="@+id/paraagendar_view"
        app:layout_constraintEnd_toEndOf="@+id/accodata_view"
        app:layout_constraintHorizontal_bias="0.476"
        app:layout_constraintStart_toStartOf="@+id/accodata_view"
        app:layout_constraintTop_toTopOf="@+id/accodata_view"
        app:layout_constraintVertical_bias="0.864" />

    <TextView
        android:id="@+id/textView16"
        android:layout_width="173dp"
        android:layout_height="39dp"
        android:fontFamily="@font/sfpro"
        android:text="@string/mais_opcs"
        android:textAlignment="center"
        android:textColor="@color/white"
        android:textSize="20sp"
        app:layout_constraintBottom_toTopOf="@+id/paraagendmnts_view"
        app:layout_constraintEnd_toEndOf="@+id/accopts_view"
        app:layout_constraintHorizontal_bias="0.47"
        app:layout_constraintStart_toEndOf="@+id/accodata_view"
        app:layout_constraintTop_toTopOf="@+id/accopts_view"
        app:layout_constraintVertical_bias="0.865" />

    <View
        android:id="@+id/paraagendar_view"
        android:layout_width="179dp"
        android:layout_height="180dp"
        android:background="@drawable/buttonopc"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="@+id/view8"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/view8"
        app:layout_constraintVertical_bias="0.441" />

    <TextView
        android:id="@+id/shop_texto"
        android:layout_width="337dp"
        android:layout_height="61dp"
        android:fontFamily="@font/sfpro"
        android:text="@string/barbequita"
        android:textAlignment="center"
        android:textColor="@color/white"
        android:textFontWeight="@android:integer/config_longAnimTime"
        android:textSize="30sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="@+id/view8"
        app:layout_constraintEnd_toEndOf="@+id/view8"
        app:layout_constraintHorizontal_bias="0.121"
        app:layout_constraintStart_toStartOf="@+id/view8"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.74" />

    <ImageView
        android:id="@+id/icone_agendmnt"
        android:layout_width="143dp"
        android:layout_height="119dp"
        android:layout_marginStart="16dp"
        android:layout_marginBottom="56dp"
        app:layout_constraintBottom_toTopOf="@+id/view15"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/agendar_texto"
        android:layout_width="170dp"
        android:layout_height="52dp"
        android:fontFamily="@font/sfpro"
        android:text="@string/agendar_novo"
        android:textAlignment="center"
        android:textColor="@color/white"
        android:textSize="18sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/icone_agendmnt" />

    <TextView
        android:id="@+id/listaragendmnts_texto"
        android:layout_width="170dp"
        android:layout_height="52dp"
        android:layout_marginEnd="8dp"
        android:layout_marginBottom="12dp"
        android:fontFamily="@font/sfpro"
        android:text="@string/ver_agendamentos"
        android:textAlignment="center"
        android:textColor="@color/white"
        android:textSize="20sp"
        app:layout_constraintBottom_toBottomOf="@+id/paraagendmnts_view"
        app:layout_constraintEnd_toEndOf="@+id/paraagendmnts_view" />

</androidx.constraintlayout.widget.ConstraintLayout>